<idea-plugin>
    <id>com.automation.iosccevaluator</id>
    <name>ioscc-evaluator</name>
    <vendor>mohagan9</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.automation.iosccevaluator.services.MyApplicationService"/>
        <projectService serviceImplementation="com.automation.iosccevaluator.services.MyProjectService"/>
    </extensions>
</idea-plugin>
